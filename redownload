echo "Removing old version"
rm $PREFIX/bin/setting
echo "Downloading new version "
mkdir $HOME/tempaa
cd $HOME/tempaa
git clone https://github.com/SathvikKS/T-Setting.git
cd $HOME
rm .bashrc
rm .pwdd
rm -rf T-Setting
cp $HOME/tempaa/T-Setting -r  $HOME
rm -rf tempaa
cd T-Setting
chmod +x setup
./setup
cd $HOME
echo "Done updating the new script from github"
echo "You can now work with it normally"
sleep 4
clear
echo "You can execute the script by type --> setting"
