cp $HOME/T-Setting/bashrc $HOME/.bashrc
cd
touch .pwdd
touch .clr
touch .pstore
cd T-Setting
chmod +x *
apt-get install nano -y
apt-get install nodejs -y
pkg install p7zip
chmod +x setting
chmod +x redownload
chmod +x password
chmod +x colour
chmod +x motd
chmod +x version
chmod +x backup
clear
unlink /data/data/com.termux/files/usr/bin/setting
rm /data/data/com.termux/files/usr/bin/setting
clear
echo "You can execute this script anytime by typing --> setting"
sleep 4
